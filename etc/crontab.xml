<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="swissup_marketplace_job_run" instance="Swissup\Marketplace\Cron\ProcessQueue" method="execute">
            <schedule>* * * * *</schedule>
        </job>
        <job name="swissup_marketplace_job_cleanup" instance="Swissup\Marketplace\Cron\CleanupQueue" method="execute">
            <schedule>45 2 * * *</schedule>
        </job>
    </group>
</config>
